{
  "name": "zxui",
  "version": "1.0.0",
  "description": "A Vue.js 3.0 UI Component Library",
  "main": "index.ts",
  "module": "index.ts",
  "types": "index.ts",
  "unpkg": "dist/index.full.js",
  "jsdelivr": "dist/index.full.js",
  "exports": {
    ".": {
      "types": "./index.ts",
      "import": "./index.ts",
      "require": "./index.ts"
    },
    "./es": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs"
    },
    "./lib": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js"
    },
    "./es/*": "./es/*",
    "./lib/*": "./lib/*",
    "./dist/*": "./dist/*",
    "./theme-chalk/*": "./theme-chalk/*",
    "./package.json": "./package.json"
  },
  "keywords": [
    "zxui",
    "vue",
    "vue3",
    "component",
    "components",
    "ui"
  ],
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@zxui/components": "workspace:*",
    "@zxui/constants": "workspace:*",
    "@zxui/hooks": "workspace:*",
    "@zxui/theme-chalk": "workspace:*",
    "@zxui/utils": "workspace:*",
    "@element-plus/icons-vue": "^2.3.1"
  }
}
